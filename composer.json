{
    "name": "easyengine/auth-command",
    "description": "Configure HTTP Authentication and whitelisting for EasyEngine site",
    "type": "ee-cli-package",
    "homepage": "https://github.com/easyengine/auth-command",
    "license": "MIT",
    "authors": [],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "": "src/",
            "\\EE\\Model\\": "src/db/"
        },
        "files": [
            "src/auth-utils.php",
            "src/helper/hooks.php",
            "auth-command.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "bundled": true,
        "commands": [
            "auth",
            "auth create",
            "auth delete",
            "auth list",
            "auth update",
            "auth whitelist"
        ]
    }
}
